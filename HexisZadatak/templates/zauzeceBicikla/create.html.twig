{% extends 'base.html.twig' %}

{% block body %}
  <div class="container">
    <h1 style="text-align: center">Upišite zauzeće bicikla</h1>

    {% for message in app.flashes('message') %}
      <div class="alert alert-danger">
        {{message}}
      </div>
    {% endfor %}

    <div style="width:500px; margin:auto; margin-top: 30px">
      {{form(form)}}
    </div>

    <a class="btn btn-success" href="{{path('zauzeceBicikla.view')}}">Natrag</a>
  </div>
{% endblock %}
